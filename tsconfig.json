{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "paths": {
    "@public/*": ["public/*"],
    "@assets/*": ["public/assets/*"],
    "@fonts/*": ["public/Font/*"],
    "@components/*": ["src/components/*"],
    "@utils/*": ["src/utils/*"]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "components/Navbar/Navbar.tsx", "components/Navbar/Mylinks.tsx", "components/Navbar/NavLink.tsx", "components/SidebarData.jsx", "components/Sidebar.jsx"  ],
  "exclude": [
    "node_modules"
  ],
}
